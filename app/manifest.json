{
  "schemaVersion": "1.3",
  "acapPackageConf": {
      "setup": {
          "appName": "weatherflow",
          "friendlyName": "WeatherFlow Overlay",
          "vendor": "Tristan Horn",
          "embeddedSdkVersion": "2.0",
          "user": {
              "group": "sdk",
              "username": "sdk"
          },
          "vendorUrl": "https://github.com/tris",
          "runMode": "respawn",
          "version": "1.0.1"
      },
      "configuration":  {
        "settingPage": "index.html",
        "paramConfig": [
          {
            "name": "Token",
            "type": "textarea",
            "default": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
          },
          {
            "name": "DeviceID",
            "type": "textarea",
            "default": "123456"
          },
          {
            "name": "DisplayName",
            "type": "textarea",
            "default": "My Station"
          },
          {
            "name": "Position",
            "type": "enum:topleft,topright,bottomleft,bottomright",
            "default": "bottomleft"
          }
        ]
      }
  }
}
